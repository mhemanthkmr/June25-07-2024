# linear search in list
def linear_search(arr, key):
    for index, value in enumerate(arr):
        if value == key:
            return index  
    return -1  

# Example usage:
my_list = [10, 5, 7, 2, 1, 6]
key_value = 7

result = linear_search(my_list, key_value)
if result != -1:
    print(f"Target value {key_value} found at index {result}.")
else:
    print(f"Target value {key_value} not found in the list.")
